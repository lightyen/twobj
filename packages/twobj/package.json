{
	"name": "twobj",
	"version": "0.0.4",
	"homepage": "https://github.com/lightyen/twobj",
	"author": {
		"name": "lightyen",
		"url": "https://github.com/lightyen"
	},
	"repository": {
		"directory": "https://github.com/lightyen/twobj/tree/main/packages/twobj"
	},
	"types": "./types/index.d.ts",
	"exports": {
		".": {
			"require": "./index.cjs",
			"import": "./index.mjs"
		}
	},
	"scripts": {
		"compile": "del ./index.* && rollup -c",
		"types": "del ./types/** !./types/tw-prop.d.ts && tsc --project tsconfig.types.json --declaration --emitDeclarationOnly --outDir types",
		"format": "prettier --config ../../.prettierrc.yml --write {src,types}/**/*.ts",
		"build": "npm run compile && npm run types && npm run format"
	},
	"files": [
		"types",
		"index.cjs",
		"index.mjs",
		"README.md"
	]
}
