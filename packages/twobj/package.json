{
	"name": "twobj",
	"version": "0.0.30",
	"homepage": "https://github.com/lightyen/twobj",
	"author": {
		"name": "lightyen",
		"url": "https://github.com/lightyen"
	},
	"repository": {
		"directory": "https://github.com/lightyen/twobj/tree/main/packages/twobj"
	},
	"exports": {
		".": {
			"require": "./index.js",
			"import": "./index.mjs"
		},
		"./parser": {
			"require": "./parser/index.js",
			"import": "./parser/index.mjs"
		},
		"./config/defaultConfig": {
			"require": "./config/defaultConfig.js",
			"import": "./config/defaultConfig.mjs"
		}
	},
	"scripts": {
		"compile": "del ./index.* ./parser ./config && tsup",
		"types": "del *.d.ts **/*.d.ts && tsc --project tsconfig.types.json",
		"format": "prettier --config ../../.prettierrc.yml --write *.*ts **/*.*ts",
		"build": "npm run compile && npm run types && npm run format"
	},
	"files": [
		"parser",
		"config",
		"index.js",
		"index.mjs",
		"*.d.ts"
	],
	"devDependencies": {
		"@types/cssesc": "3.0.0",
		"cssesc": "3.0.0",
		"parsel-js": "1.0.2",
		"postcss": "8.4.16",
		"tailwindcss": "3.1.8",
		"tailwind-types": "3.2.0"
	}
}
