{
	"name": "twobj",
	"version": "0.0.4",
	"homepage": "https://github.com/lightyen/twobj",
	"author": {
		"name": "lightyen",
		"url": "https://github.com/lightyen"
	},
	"repository": {
		"directory": "https://github.com/lightyen/twobj/tree/main/packages/twobj"
	},
	"exports": {
		".": {
			"require": "./index.cjs",
			"import": "./index.mjs"
		},
		"./parser": {
			"require": "./parser/index.cjs",
			"import": "./parser/index.mjs"
		}
	},
	"scripts": {
		"compile": "del ./index.* ./parser/index.* && rollup -c",
		"types": "del *.d.ts **/*.d.ts && tsc --project tsconfig.types.json",
		"format": "prettier --config ../../.prettierrc.yml --write **/*.ts",
		"build": "npm run compile && npm run types && npm run format"
	},
	"files": [
		"parser",
		"index.cjs",
		"index.mjs",
    "*.d.ts"
	]
}
