{
	"name": "babel-plugin-twobj",
	"version": "0.0.17",
	"homepage": "https://github.com/lightyen/babel-plugin-twobj",
	"author": {
		"name": "lightyen",
		"url": "https://github.com/lightyen"
	},
	"repository": {
		"directory": "https://github.com/lightyen/twobj/tree/main/packages/babel-plugin-twobj"
	},
	"exports": {
		".": {
			"require": "./index.js",
			"import": "./index.mjs"
		}
	},
	"scripts": {
		"compile": "del ./index.* && tsup",
		"types": "del *.d.ts *.d.cts *.d.mts **/*.d.*ts !./types/*.d.ts && tsc --project tsconfig.types.json",
		"format": "prettier --config ../../.prettierrc.yml --write *.*ts **/*.*ts",
		"build": "npm run compile && npm run types && npm run format"
	},
	"peerDependencies": {
		"twobj": "*"
	},
	"devDependencies": {
		"@babel/core": "7.18.13",
		"@babel/generator": "7.18.13",
		"@babel/preset-env": "7.18.10",
		"@babel/preset-react": "7.18.6",
		"@babel/preset-typescript": "7.18.6",
		"@babel/types": "^7.18.13",
		"@emotion/babel-plugin": "11.10.2",
		"@emotion/react": "11.10.4",
		"@emotion/styled": "11.10.4",
		"@types/babel__core": "^7.1.19",
		"@types/babel-plugin-tester": "9.0.5",
		"@types/react": "18.0.18",
		"babel-plugin-tester": "10.1.0",
		"postcss": "8.4.16",
		"react": "18.2.0",
		"tailwind-types": "3.2.0",
		"tailwindcss": "3.1.8",
		"twobj": "../twobj"
	},
	"files": [
		"types",
		"index.*",
		"*.d.ts"
	]
}
