{
	"name": "babel-plugin-twobj",
	"version": "0.0.1",
	"homepage": "https://github.com/lightyen/babel-plugin-twobj",
	"author": {
		"name": "lightyen",
		"url": "https://github.com/lightyen"
	},
	"repository": {
		"directory": "https://github.com/lightyen/twobj/tree/main/packages/babel-plugin-twobj"
	},
	"types": "./types/plugin.d.mts",
	"exports": {
		".": {
			"require": "./plugin.cjs",
			"import": "./plugin.mjs"
		},
		"./require": {
			"require": "./plugin.cjs",
			"types": "./types/plugin.d.cts"
		}
	},
	"scripts": {
		"compile": "del ./index.* && rollup -c",
		"types": "del ./types && tsc --project tsconfig.types.json --declaration --emitDeclarationOnly --outDir types",
		"format": "prettier --config ../../.prettierrc.yml --write {src,types}/**/*.ts",
		"build": "npm run compile && npm run types && npm run format"
	},
	"peerDependencies": {
		"twobj": "^0.0.2"
	},
	"devDependencies": {
		"twobj": "../twobj",
		"@babel/types": "^7.18.10",
		"@types/babel__core": "^7.1.19"
	},
	"files": [
		"types",
		"plugin.*",
		"README.md"
	]
}
