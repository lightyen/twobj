{
  "name": "twobj-intellisense",
  "displayName": "Twobj IntelliSense",
  "markdownDescription": "IntelliSense extension for twobj",
  "version": "2.3.7",
  "author": "lightyen",
  "license": "MIT",
  "private": true,
  "icon": "media/icon.png",
  "homepage": "https://github.com/lightyen/twobj/tree/main/tools/vscode-extension",
  "repository": {
    "type": "git",
    "url": "https://github.com/lightyen/twobj.git"
  },
  "publisher": "lightyen",
  "categories": [
    "Programming Languages"
  ],
  "keywords": [
    "tailwindcss",
    "autocomplete",
    "intellisense",
    "css-in-js",
    "react"
  ],
  "engines": {
    "vscode": "^1.70.0"
  },
  "galleryBanner": {
    "color": "#DFDFDF",
    "theme": "light"
  },
  "activationEvents": [
    "onLanguage:javascript",
    "onLanguage:typescript",
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "main": "./dist/extension.js",
  "browser": "./dist/web/extension.js",
  "scripts": {
    "doit": "pnpm update --interactive --latest",
    "analyze": "cross-env NODE_ENV=production webpack --config webpack.analyzer.js",
    "build": "cross-env NODE_ENV=production webpack --config webpack.config.js",
    "watch-node": "cross-env NODE_ENV=development webpack --config webpack.node.js --watch",
    "watch-web": "cross-env NODE_ENV=development webpack --config webpack.web.js --watch",
    "open-in-browser": "vscode-test-web --extensionDevelopmentPath=. ~/react-emotion",
    "vscode:prepublish": "npm run build",
    "format": "prettier --write \"**/*.{ts,json,yml}\"",
    "check-format": "prettier --check \"**/*.{ts,json,yml}\"",
    "package": "vsce package --no-dependencies"
  },
  "devDependencies": {
    "@formatjs/intl": "2.6.9",
    "@swc-node/register": "1.6.3",
    "@swc/core": "1.3.46",
    "@types/node": "16",
    "@types/source-map-support": "0.5.6",
    "@types/vscode": "1.70.0",
    "@types/webpack-env": "1.18.0",
    "@typescript-eslint/eslint-plugin": "5.57.1",
    "@typescript-eslint/parser": "5.57.1",
    "@vscode/test-web": "0.0.40",
    "@vscode/vsce": "2.18.0",
    "@yarnpkg/pnp": "4.0.0-rc.42",
    "assert": "2.0.0",
    "chokidar": "3.5.3",
    "clean-webpack-plugin": "4.0.0",
    "copy-webpack-plugin": "11.0.0",
    "cross-env": "7.0.3",
    "culori": "3.1.0",
    "esbuild": "0.17.15",
    "eslint": "8.37.0",
    "eslint-webpack-plugin": "4.0.0",
    "events": "3.3.0",
    "fork-ts-checker-webpack-plugin": "8.0.0",
    "fsevents": "2.3.2",
    "fuse.js": "6.6.2",
    "js-yaml-loader": "1.2.2",
    "node-loader": "2.0.0",
    "object-hash": "3.0.0",
    "path-browserify": "1.0.1",
    "postcss": "8.4.21",
    "postcss-js": "4.0.1",
    "postcss-selector-parser": "6.0.11",
    "prettier": "3.0.0-alpha.6",
    "process": "0.11.10",
    "source-map-support": "0.5.21",
    "swc-loader": "0.2.3",
    "terser-webpack-plugin": "5.3.7",
    "ts-paths-resolve-plugin": "5.9.1",
    "twobj": "../../packages/twobj",
    "typescript": "5.0.3",
    "typescript-paths": "1.5.0",
    "util": "0.12.5",
    "vscode-css-languageservice": "6.2.4",
    "vscode-languageclient": "8.1.0",
    "vscode-languageserver": "8.1.0",
    "vscode-languageserver-textdocument": "1.0.8",
    "vscode-languageserver-types": "3.17.3",
    "vscode-uri": "3.0.7",
    "webpack": "5.78.0",
    "webpack-cli": "5.0.1",
    "webpack-merge": "5.8.0",
    "webpack-visualizer-plugin2": "1.0.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "tw",
        "extensions": [
          ".twd"
        ],
        "configuration": "./syntaxes/language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "tw",
        "scopeName": "source.tw",
        "path": "./syntaxes/tw.json"
      },
      {
        "injectTo": [
          "source.tw"
        ],
        "scopeName": "source.css.scss.tw",
        "path": "./syntaxes/scss.tmLanguage.json"
      },
      {
        "injectTo": [
          "source.tsx",
          "source.js.jsx",
          "source.ts",
          "source.js"
        ],
        "scopeName": "source.template.tw",
        "path": "./syntaxes/injection.json"
      },
      {
        "injectTo": [
          "source.tsx",
          "source.js.jsx"
        ],
        "scopeName": "source.tag.tw",
        "path": "./syntaxes/injectionJsx.json"
      }
    ],
    "configuration": {
      "title": "Twobj IntelliSense",
      "properties": {
        "twobj.colorDecorators": {
          "markdownDescription": "%ext.settings.colorDecorators%",
          "additionalItems": false,
          "type": "string",
          "enum": [
            "on",
            "off",
            "inherit"
          ],
          "default": "inherit"
        },
        "twobj.references": {
          "markdownDescription": "%ext.settings.references%",
          "type": "boolean",
          "default": true
        },
        "twobj.diagnostics": {
          "markdownDescription": "%ext.settings.diagnostics%",
          "type": "boolean",
          "default": true
        },
        "twobj.preferVariantWithParentheses": {
          "type": "boolean",
          "default": false
        },
        "twobj.fallbackDefaultConfig": {
          "type": "boolean",
          "default": true
        },
        "twobj.enabled": {
          "markdownDescription": "%ext.settings.enabled%",
          "type": "boolean",
          "default": true
        },
        "twobj.rootFontSize": {
          "type": "number",
          "default": 16
        },
        "twobj.otherLanguages": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": []
        },
        "twobj.hoverColorHint": {
          "type": "string",
          "enum": [
            "none",
            "hex",
            "rgb",
            "hsl"
          ],
          "default": "none"
        },
        "twobj.hoverUtility": {
          "type": "string",
          "enum": [
            "showVariants",
            "default"
          ],
          "default": "default"
        },
        "twobj.minimumContrastRatio": {
          "type": "number"
        },
        "twobj.logLevel": {
          "type": "string",
          "description": "%ext.settings.logging.level%",
          "enum": [
            "none",
            "error",
            "warning",
            "info",
            "debug",
            "trace"
          ],
          "default": "info"
        },
        "twobj.documentColors": {
          "type": "boolean",
          "default": false
        },
        "twobj.importLabels": {
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "twobj"
          ]
        }
      }
    },
    "commands": [
      {
        "command": "twobj.preview",
        "title": "Twobj: Preview"
      }
    ]
  }
}
